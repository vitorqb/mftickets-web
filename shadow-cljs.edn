{:source-paths ["src/cljs" "test/cljs"]
 :dependencies
 [[reagent "0.8.1"]
  [reagent-utils "0.3.3"]
  [metosin/reitit "0.3.7"]
  [pez/clerk "1.0.0"]
  [venantius/accountant "0.2.4" :exclusions [org.clojure/tools.reader]]
  [cider/cider-nrepl "0.22.3"]
  [nrepl "0.6.0"]
  [cider/piggieback "0.4.1"]
  [refactor-nrepl "2.5.0-SNAPSHOT"]
  [cljs-http "0.1.46"]
  [binaryage/devtools "0.9.10"]
  [com.rpl/specter "1.1.2"]]
 
 :builds {:app {:target     :browser
                :output-dir "resources/public/js"
                :asset-path "/js"
                :modules    {:app {:entries [mftickets-web.core]}}
                :devtools   {:after-load mftickets-web.core/mount-root}}
          :test {:target     :karma
                 :output-to  "resources/public/test-build/js/karma-test-build.js"}}

 :dev-http {4000 {:root "resources/public"}}

 :nrepl {:port 6500}}
